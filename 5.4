PImage picture;
PImage picture2;
PImage picture3;
PImage picture4;

void setup(){
  size(1000, 600);
  
  picture = loadImage("test.jpg");
  picture2 = loadImage("test2.jpg");
  picture3 = loadImage("test3.jpg");
  picture4 = loadImage("test4.jpg");
}

void draw(){
  background(255);
  
  blendMode(SUBTRACT);
  image(picture,100,200,200,200);
  image(picture2,300,200,200,200);
  image(picture3,500,200,200,200);
  image(picture4,700,200,200,200);
}
