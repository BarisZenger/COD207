int x1 = 0;
int y1 = 0;
int w1 = 100;
int h1 = 100;
int x2 = 375;
int y2 = 375;
int w2 = 50;
int h2 = 50;

PFont startFont;

void setup(){
  size(800,800);
  
  startFont = createFont("BLACEB__.TTF", 32);
}

void draw(){
  background(255);
  noStroke();
  
  x1 = mouseX - 50;
  y1 = mouseY - 50;
  
  fill(#F39C12);
  rect(x1,y1,w1,h1);
  
  fill(#27AE60);
  rect(x2,y2,w2,h2);
  
  if(x1 + w1 > x2 && x1 < x2 + w2){
    if(y1 + h1 > y2 && y1 < y2 + h2){
    fill(0);
    textFont(startFont);
    textSize(90);
    textAlign(CENTER);
    text("GAME OVER", width * 0.5, height*0.5);
    stop();
}
}
}
